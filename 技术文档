```sql
create table Students
```
create table Students

use stu;
drop table student cascade;
create table Students 
(  
    Sno int(20) primary key,
    Sname char(20) ,
    sex char(4),
    age int(4),
    Id int(20),
    foreign key (Id) references School (Id)
)  ENGINE=InnoDB DEFAULT CHARSET=utf8;

create table Courses 
(  
    Cno int(20),  
    Cname varchar(50),    
    primary key (Cno)  
) ENGINE=InnoDB DEFAULT CHARSET=utf8;  
use stu;   
create table SC
(  
    Sno int(20),  
    Cno int(20),      
    score int,  
    primary key (Sno,Cno),
    foreign key (Sno) references Students (Sno),
    foreign key (Cno) references Courses (Cno)
) ENGINE=InnoDB DEFAULT CHARSET=utf8;  
  
create table School
(  
    Id int(20),  
    sna varchar(50), 
    pna varchar(50),
    primary key (Id)  
) ENGINE=InnoDB DEFAULT CHARSET=utf8;
